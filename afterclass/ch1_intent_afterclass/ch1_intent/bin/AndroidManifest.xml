<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="qdu.szmt.ch1_intent"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="qdu.szmt.ch1_intent.Myintent">
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="qdu.szmt.ch1_intent.Implicit_act">
            <!-- intent filter三要素 action、category、data -->
            <intent-filter>
                <action android:name="myaction"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:scheme="myqdu" 
                    android:host="datatech.rjgc"/>
            </intent-filter>
        </activity>
        <activity android:name="qdu.szmt.ch1_intent.Act_explicit"></activity>
    </application>

</manifest>
